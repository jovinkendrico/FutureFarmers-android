                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp">

                    <TextView
                        android:id="@+id/tvTingkatKeasaman"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:text="Tingkat Keasaman"
                        android:textColor="@color/abu"
                        android:textSize="20sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/tvAngkaTingkatKeasaman"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="5dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginBottom="10dp"

                        android:textColor="@color/black"
                        android:textSize="56sp"
                        app:layout_constraintBottom_toTopOf="@id/progressBarPh"
                        app:layout_constraintEnd_toStartOf="@+id/tvAngkaPh"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvTingkatKeasaman" />

                    <TextView
                        android:id="@+id/tvAngkaPh"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="20dp"
                        android:text="pH"
                        android:textColor="@color/abu"
                        android:textSize="32sp"
                        app:layout_constraintBottom_toBottomOf="@+id/tvAngkaTingkatKeasaman"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@id/tvAngkaTingkatKeasaman"
                        app:layout_constraintTop_toTopOf="@+id/tvAngkaTingkatKeasaman" />
                    <ProgressBar
                        android:id="@+id/progressBarPh"
                        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"

                        android:max="140"
                        android:progress="0"
                        android:progressDrawable="@drawable/stats_progress_bar"
                        app:layout_constraintBottom_toTopOf="@id/tv0"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tvAngkaTingkatKeasaman" />

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:src="@drawable/ph"
                        app:layout_constraintBottom_toTopOf="@+id/tvAngkaTingkatKeasaman"
                        app:layout_constraintStart_toEndOf="@+id/tvTingkatKeasaman"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/tv0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginBottom="10dp"
                        android:text="0"
                        android:textColor="@color/abu"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/progressBarPh"
                        app:layout_constraintVertical_bias="0.0" />

                    <TextView
                        android:id="@+id/tv14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="20dp"
                        android:layout_marginBottom="10dp"
                        android:text="14"

                        android:textColor="@color/abu"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/progressBarPh"
                        app:layout_constraintVertical_bias="0.0" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

