<?xml version="1.0" encoding="utf-8"?>
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/main"
        android:clipToPadding="false"
        tools:context=".ui.main.HomeFragment"
        android:backgroundTint="@color/background">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:backgroundTint="@color/background"
            android:paddingBottom="90dp">


            <TextView
                android:id="@+id/tvSelamatDatang"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="Selamat Datang!"
                android:textColor="@color/abu"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tvNama"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="Ayo Pantau Tumbuh si Hidro"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvSelamatDatang" />

            <androidx.cardview.widget.CardView
                android:id="@+id/cvSedangMenanam"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"

                app:cardCornerRadius="20dp"
                app:cardElevation="4dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvNama">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp">

                    <TextView
                        android:id="@+id/tvSedangMenanam"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:text="Sedang Menanam"
                        android:textColor="@color/abu"
                        android:textSize="20sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageView
                        android:id="@+id/ivGambarSayur"
                        android:layout_width="135dp"
                        android:layout_height="134dp"
                        android:layout_margin="10dp"
                        android:src="@drawable/lettuce"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/tvSayur"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tvSedangMenanam"
                         />

                    <TextView
                        android:id="@+id/tvSayur"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="10dp"
                        android:textColor="@color/black"
                        android:textSize="32sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toEndOf="@+id/ivGambarSayur"
                        app:layout_constraintTop_toBottomOf="@+id/tvSedangMenanam" />

                    <TextView
                        android:id="@+id/tvUmurTanaman"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:text="Umur"
                        android:textColor="@color/abu"
                        android:textSize="12sp"
                        app:layout_constraintEnd_toStartOf="@id/tvPanenDalam"
                        app:layout_constraintStart_toEndOf="@+id/ivGambarSayur"
                        app:layout_constraintTop_toBottomOf="@+id/tvSayur" />

                    <TextView
                        android:id="@+id/tvPanenDalam"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:text="Panen Dalam"
                        android:textColor="@color/abu"
                        android:textSize="12sp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@id/tvUmurTanaman"
                        app:layout_constraintTop_toBottomOf="@+id/tvSayur" />

                    <TextView
                        android:id="@+id/tvUmur"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="5dp"
                        android:textColor="@color/black"
                        android:textSize="16sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/tvHariUmur"
                        app:layout_constraintStart_toEndOf="@id/ivGambarSayur"
                        app:layout_constraintTop_toBottomOf="@+id/tvUmurTanaman" />

                    <TextView
                        android:id="@+id/tvPanen"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:textSize="16sp"
                        android:layout_marginEnd="5dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@id/tvHari"
                        app:layout_constraintStart_toStartOf="@+id/tvPanenDalam"
                        app:layout_constraintTop_toBottomOf="@+id/tvPanenDalam" />
                    <TextView
                        android:id="@+id/tvHari"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="10dp"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        android:text="Hari"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@id/tvPanen"
                        app:layout_constraintTop_toBottomOf="@+id/tvPanenDalam" />

                    <TextView
                        android:id="@+id/tvHariUmur"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="Hari"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/tvUmurTanaman"
                        app:layout_constraintStart_toEndOf="@id/tvUmur"
                        app:layout_constraintTop_toBottomOf="@+id/tvUmurTanaman" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvSuhu"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="5dp"
                app:cardCornerRadius="20dp"
                app:layout_constraintEnd_toStartOf="@id/cvKelembapan"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cvSedangMenanam">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp">

                    <TextView
                        android:id="@+id/tvSuhu"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:text="Suhu"
                        android:textColor="@color/abu"
                        android:textSize="20sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <ProgressBar
                        android:id="@+id/progressBarSuhu"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="140dp"
                        android:layout_height="140dp"
                        android:layout_marginBottom="10dp"
                        android:indeterminate="false"
                        android:max="500"
                        android:progress="0"
                        android:background="@drawable/stats_progress_drawable"
                        android:progressDrawable="@drawable/stats_progress"
                        android:scaleX="1"
                        android:scaleY="1"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.506"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tvSuhu"
                        app:layout_constraintVertical_bias="0.0" />

                    <TextView
                        android:id="@+id/tvAngkaSuhu"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginBottom="10dp"
                        android:text=""
                        android:textAlignment="textEnd"
                        android:textColor="@color/black"
                        android:textSize="28sp"
                        app:layout_constraintBottom_toBottomOf="@+id/progressBarSuhu"
                        app:layout_constraintEnd_toStartOf="@+id/tvDerajat"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="@+id/progressBarSuhu"
                        app:layout_constraintTop_toTopOf="@+id/progressBarSuhu" />

                    <TextView
                        android:id="@+id/tvDerajat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:text="°"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="28sp"
                        app:layout_constraintBottom_toBottomOf="@+id/progressBarSuhu"
                        app:layout_constraintEnd_toStartOf="@id/tvDerajatc"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/tvAngkaSuhu"
                        app:layout_constraintTop_toTopOf="@+id/progressBarSuhu" />

                    <TextView
                        android:id="@+id/tvDerajatc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="30dp"
                        android:layout_marginBottom="10dp"
                        android:text="c"
                        android:textAlignment="textStart"
                        android:textColor="@color/abu"
                        android:textSize="28sp"
                        app:layout_constraintBottom_toBottomOf="@+id/progressBarSuhu"

                        app:layout_constraintEnd_toEndOf="@+id/progressBarSuhu"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/tvDerajat"
                        app:layout_constraintTop_toTopOf="@+id/progressBarSuhu" />
                    <ImageView
                        android:id="@+id/imageView5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="10dp"
                        android:src="@drawable/temp"
                        app:layout_constraintBottom_toTopOf="@+id/progressBarSuhu"
                        app:layout_constraintStart_toEndOf="@+id/tvSuhu"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvKelembapan"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="20dp"
                app:cardCornerRadius="20dp"
                app:layout_constraintBottom_toBottomOf="@+id/cvSuhu"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/cvSuhu"
                app:layout_constraintTop_toBottomOf="@id/cvSedangMenanam">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp">

                    <TextView
                        android:id="@+id/tvKelembapan"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:text="Kelembapan"
                        android:textColor="@color/abu"
                        android:textSize="20sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                    <ProgressBar
                        android:id="@+id/progressBarKelembapan"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="140dp"
                        android:layout_height="140dp"
                        android:layout_marginBottom="10dp"
                        android:indeterminate="false"
                        android:max="90"
                        android:progress="0"
                        android:background="@drawable/stats_progress_drawable"

                        android:progressDrawable="@drawable/stats_progress"
                        android:scaleX="1"
                        android:scaleY="1"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tvKelembapan"
                        app:layout_constraintVertical_bias="0.0" />

                    <TextView
                        android:id="@+id/tvAngkaKelembapan"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginBottom="10dp"
                        android:text=""
                        android:textAlignment="textEnd"
                        android:textColor="@color/black"
                        android:textSize="28sp"
                        app:layout_constraintBottom_toBottomOf="@+id/progressBarKelembapan"
                        app:layout_constraintEnd_toStartOf="@id/tvKelembapanPersen"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="@+id/progressBarKelembapan"
                        app:layout_constraintTop_toTopOf="@+id/progressBarKelembapan" />

                    <TextView
                        android:id="@+id/tvKelembapanPersen"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="45dp"
                        android:layout_marginBottom="10dp"
                        android:text="%"
                        android:textAlignment="textStart"
                        android:textColor="@color/abu"
                        android:textSize="20sp"
                        app:layout_constraintBottom_toBottomOf="@+id/progressBarKelembapan"
                        app:layout_constraintEnd_toEndOf="@+id/progressBarKelembapan"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@id/tvAngkaKelembapan"
                        app:layout_constraintTop_toTopOf="@+id/progressBarKelembapan" />
                    <ImageView
                        android:id="@+id/imageView6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="10dp"
                        android:src="@drawable/kelembapan"
                        app:layout_constraintBottom_toTopOf="@+id/progressBarKelembapan"
                        app:layout_constraintStart_toEndOf="@+id/tvKelembapan"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvTingkatKeasaman"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                app:cardCornerRadius="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
